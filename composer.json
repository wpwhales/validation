{
    "name": "wpwhales/validation",
    "description": "The Illuminate Validation package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-filter": "*",
        "ext-mbstring": "*",
        "brick/math": "^0.9.3|^0.10.2|^0.11",
        "egulias/email-validator": "^3.2.5|^4.0",
        "wpwhales/collections": "dev-master",
        "wpwhales/container": "dev-master",
        "wpwhales/contracts": "dev-master",
        "wpwhales/macroable": "dev-master",
        "wpwhales/support": "dev-master",
        "wpwhales/translation": "dev-master",
        "symfony/http-foundation": "^6.3",
        "symfony/mime": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "WPWhales\\Validation\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        }
    },
    "suggest": {
        "wpwhales/database": "Required to use the database presence verifier (^11.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
